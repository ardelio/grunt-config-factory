    "auto",
    "factory"
  ],
  "author": "Anthony Sceresini",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/asceresini/grunt-config-factory/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/asceresini/grunt-config-factory/issues"
  },
  "homepage": "https://github.com/asceresini/grunt-config-factory",
  "dependencies": {
    "lodash": "^3.0.0"
  },
  "devDependencies": {
    "jshint": "^2.7.0",
    "nodeunit": "^0.9.0",
    "proxyquire": "^1.3.1"
  }
